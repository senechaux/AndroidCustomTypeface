<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:custom="http://schemas.android.com/apk/res/com.senechaux.androidcustomtypeface"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivity" >

    <com.senechaux.androidcustomtypeface.CustomButton
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/font_infinite"
        custom:typeface="@string/font_infinite" />

    <com.senechaux.androidcustomtypeface.CustomButton
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/font_jfringmaster"
        custom:typeface="@string/font_jfringmaster" />

    <com.senechaux.androidcustomtypeface.CustomButton
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/font_jfrocsol"
        custom:typeface="@string/font_jfrocsol" />

    <com.senechaux.androidcustomtypeface.CustomButton
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/font_bradbunr"
        custom:typeface="@string/font_bradbunr" />

    <com.senechaux.androidcustomtypeface.CustomTextView
        android:id="@+id/customTextView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:text="@string/font_infinite"
        custom:typeface="@string/font_infinite" />

    <com.senechaux.androidcustomtypeface.CustomTextView
        android:id="@+id/customTextView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:text="@string/font_jfringmaster"
        custom:typeface="@string/font_jfringmaster" />

    <com.senechaux.androidcustomtypeface.CustomTextView
        android:id="@+id/customTextView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:text="@string/font_jfrocsol"
        custom:typeface="@string/font_jfrocsol" />

    <com.senechaux.androidcustomtypeface.CustomTextView
        android:id="@+id/customTextView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:text="@string/font_bradbunr"
        custom:typeface="@string/font_bradbunr" />

</LinearLayout>